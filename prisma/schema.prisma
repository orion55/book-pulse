generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Books {
  id       Int    @id @default(autoincrement())
  authorId Int
  bookId   Int
  title    String

  @@index([authorId])
}
